{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Health Monitoring Schema",
    "type": "object",
    "properties": {
        "health_monitoring": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "datetime": { "type": "string", "format": "date-time" },
                    "measurement_type": { "type": "string" },
                    "value": {},
                    "notes": { "type": "string" }
                },
                "required": ["datetime", "measurement_type", "value"]
            }
        }
    },
    "required": ["health_monitoring"]
}
